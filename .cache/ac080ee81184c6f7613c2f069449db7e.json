n                  pets\n              })\n          });\n  }\n\n  render() {\n      return(\n              <div className=\"search\">\n                  <SearchBox search={this.search}/>\n                  {this.state.pets.map(pet =>{\n                      let breed;\n                      if(Array.isArray(pet.breeds.breed)){\n                          breed = pet.breeds.breed.join(\", \");\n                      }else{\n                          breed = pet.breeds.breed;\n                      }\n\n                      return(\n                      <Pet \n                          key={pet.id} \n                          animal ={pet.animal} \n                          name = {pet.name} \n                          breed = {breed}\n                          media = {pet.media}\n                          location = {`${pet.contact.city}, ${pet.contact.state}`}\n                          id={pet.id}\n                          \n                          />\n                      );\n                  })}\n            </div>\n      );\n  }\n}\n\nexport default function ResultsWithContext(props){\n  return (\n      <Consumer>\n          {context => <Results {...props} searchParams={context}/>}\n      </Consumer>\n  );\n}\n\n"},"lineCount":null}},"hash":"233017dfa06665a0807dcf05715714ba","cacheData":{"env":{"API_KEY":"3f36ed773da413b044b6e8fff73a324c","API_SECRET":"338bc1b781abc1f3fd63d83b7d009e3e"}}}